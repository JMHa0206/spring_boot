<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.kedu.study.dao.BoardFileDAO">
    
    <insert id="insertBoardFile" parameterType="com.kedu.study.dto.BoardFileDTO">
        INSERT INTO files (parent_post, b_oriname, b_sysname)
        VALUES (#{parent_post}, #{b_oriname}, #{b_sysname});
    </insert>
</mapper>